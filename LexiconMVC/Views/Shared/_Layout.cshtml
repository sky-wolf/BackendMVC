<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
    <base href="/" />
</head>
<body>
    <div class="container">
        <header>
            <nav class="navbar navbar-expand-lg bg-light">
                <div class="container-fluid">
                    <a class="navbar-brand mb-0 h1" asp-controller="Home" asp-action="Index">Chili Case</a>
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                    MVC Basic
                                </a>
                                <ul class="dropdown-menu">
                                    <li class="nav-item">
                                        <a class="dropdown-item" asp-controller="Home" asp-action="About">About</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="dropdown-item" asp-controller="Home" asp-action="Contact">Contact</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="dropdown-item" asp-controller="Home" asp-action="Prodjects">Prodjects</a>
                                    </li>
                                    <li><hr class="dropdown-divider"></li>
                                    <li class="nav-item">
                                        <a class="dropdown-item" asp-controller="Feber" asp-action="Check">Feber</a>
                                    </li>
                                    <li><hr class="dropdown-divider"></li>
                                    <li class="nav-item">
                                        <a class="dropdown-item" asp-controller="Guessing" asp-action="Guess">Guessing game</a>
                                    </li>
                                </ul>
                            </li>

                            <li class="nav-item">
                                <a class="nav-link" asp-controller="People" asp-action="Index">People</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-controller="Ajax" asp-action="Index">Ajax</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-controller="PeopleDB" asp-action="Index">Database</a>
                            </li>
                            
                        </ul>
                    </div>
                </div>
            </nav>
        </header>
        <div class="container-fluid">
            @RenderBody()
        </div>
        <footer>
            <nav class="navbar bottom bg-light">
                <div class="container-fluid">
                    <p>@DateTime.Now.ToString("yy-MM-dd") Anders Lann</p>
                </div>
            </nav>
        </footer>
    </div>
    <script src="https://code.jquery.com/jquery-3.6.1.min.js" integrity="sha256-o88AwQnZB+VDvE9tvIXrMQaPlFFSUTR+nldQm1LuPXQ=" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>
</body>
</html>
